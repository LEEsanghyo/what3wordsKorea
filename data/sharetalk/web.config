<?xml version="1.0" encoding="UTF-8"?>
<configuration>
    <system.webServer>
        <defaultDocument>
            <files>
                <clear />
                <add value="Default.asp" />
                <add value="Default.htm" />
                <add value="index.htm" />
                <add value="index.html" />
                <add value="iisstart.htm" />
            </files>
        </defaultDocument>
        <caching enableKernelCache="true">
            <profiles>
            </profiles>
        </caching>
        <httpProtocol>
            <customHeaders>
                <add name="style" value="&lt;link rel=&quot;stylesheet&quot; href=&quot;/_include/style.css&quot; type=&quot;text/css&quot;>" />
                <add name="community" value="&lt;script type=&quot;text/javascript&quot; src=&quot;/_script/community.js&quot;>&lt;/script>" />
                <add name="Access-Control-Allow-Origin" value="*" />
            </customHeaders>
        </httpProtocol>
    </system.webServer>
</configuration>
